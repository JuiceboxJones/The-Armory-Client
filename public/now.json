{
  "version": 2,
  "alias": "squad-armory",
  "name": "squad-armory",
  "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "^/manifest.json$",
        "dest": "/manifest.json"
       },
       {
        "src": "^/site.webmanifest.json$",
        "dest": "/site.webmanifest.json"
       }
       ,
      {
        "src": ".*",
        "dest": "/index.html"
      }
  ]
}